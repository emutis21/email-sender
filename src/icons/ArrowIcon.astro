---
interface Props {
	classes?: string
}

const { classes } = Astro.props
---

<svg
	xmlns='http://www.w3.org/2000/svg'
	fill='none'
	class={classes}
	viewBox='0 0 24 24'
	stroke='currentColor'
	stroke-width='4'
>
	<path stroke-linecap='round' stroke-linejoin='round' d='M14 5l7 7m0 0l-7 7m7-7H3'></path>
</svg>

<style>
	svg {
		transition: 0.2s;
		position: relative;
		width: 15px;
		transition-delay: 0.2s;
	}
</style>
